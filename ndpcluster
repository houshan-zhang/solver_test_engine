##########

make test TEST=directed CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=7200 SETTING=ndp/ndp-1 OUTFILE=directed/a
make test TEST=directed CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=7200 SETTING=ndp/ndp-2 OUTFILE=directed/b
make test TEST=directed CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=7200 SETTING=ndp/ndp-3 OUTFILE=directed/c

make test TEST=undirected CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=7200 SETTING=ndp/ndp-1 OUTFILE=undirected/a
make test TEST=undirected CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=7200 SETTING=ndp/ndp-2 OUTFILE=undirected/b
make test TEST=undirected CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=7200 SETTING=ndp/ndp-3 OUTFILE=undirected/c

make test TEST=bidirected CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=7200 SETTING=ndp/ndp-1 OUTFILE=bidirected/a
make test TEST=bidirected CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=7200 SETTING=ndp/ndp-2 OUTFILE=bidirected/b
