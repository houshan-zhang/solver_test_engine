#!/bin/bash

make test TEST=directed CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=50 SETTING=ndp/ndp11 OUTFILE=alone/50/directed/my
make test TEST=directed CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=50 SETTING=ndp/ndp21 OUTFILE=alone/50/directed/1
make test TEST=directed CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=50 SETTING=ndp/ndp23 OUTFILE=alone/50/directed/3

make test TEST=undirected CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=50 SETTING=ndp/ndp11 OUTFILE=alone/50/undirected/my
