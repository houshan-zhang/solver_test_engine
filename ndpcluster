make test TEST=bidirected CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=50 SETTING=ndp/ndp11 OUTFILE=alone/50/bidirected/my
make test TEST=bidirected CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=50 SETTING=ndp/ndp21 OUTFILE=alone/50/bidirected/1
make test TEST=bidirected CLUSTER=on MPI=on JC=16 SOLVER=cplex_callback_ndp TIME=50 SETTING=ndp/ndp23 OUTFILE=alone/50/bidirected/3

