make test TEST=directed CLUSTER=on MPI=on JC=32 SOLVER=cplex_callback_ndp TIME=7200 SETTING=ndp/ndp3 OUTFILE=directed/3
make test TEST=undirected CLUSTER=on MPI=on JC=32 SOLVER=cplex_callback_ndp TIME=7200 SETTING=ndp/ndp3 OUTFILE=undirected/3
